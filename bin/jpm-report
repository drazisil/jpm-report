#!/usr/bin/env node

var jpm_report = require('../src/index.js')

jpm_report.checkArgs(process.argv, function cb_parse_args (err, res) {
  if (err) {
    console.log(err)
  } else {
    res = JSON.parse(res)
    console.log(res.success.total_success + ' of ' + res.success.total_tests + ' passed')
  }
})
